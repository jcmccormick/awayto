{
  "extends": "./tsconfig.paths.json",
  "include": [
    "src"
  ],
  "exclude": [
    "src/webapp"
  ],
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": false
  }
}